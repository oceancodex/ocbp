{
	"name": "oceancodex/wpsp",
	"version": "1.0.0",
	"description": "WordPress Starter Plugin",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress plugin"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.2",
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"oceancodex/wpsp-core": "^1.0"
	},
	"repositories": [
		{
			"type": "path",
			"url": "../wpsp-core",
			"options": {
				"symlink": true
			}
		}
	],
	"suggest": {
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
		}
	},
	"autoload": {
		"psr-4": {
			"WPSP\\": ""
		},
		"files": [
			"app/View/Vite.php",
			"app/Helpers/Helpers.php",
			"app/Helpers/CustomHelpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
		]
	},
	"require-dev": {
	}
}
