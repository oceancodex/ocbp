{
	"name": "oceancodex/wpsp",
	"version": "8.2.0",
	"description": "WordPress Starter Plugin",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress plugin"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.2",
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"oceancodex/wpsp-core": "^8.2.0",
		"oceancodex/wpsp-mongodb": "^8.2.0"
	},
	"require-dev": {
	},
	"repositories": [
		{
			"type": "path",
			"url": "../wpsp-core",
			"options": {
				"symlink": true
			}
		},
		{
			"type": "path",
			"url": "../wpsp-mongodb",
			"options": {
				"symlink": true
			}
		}
	],
	"config": {
		"sort-packages": true,
		"autoloader-suffix": "_wpsp_1719507007"
	},
	"autoload": {
		"psr-4": {
			"WPSP\\": "",
			"Illuminate\\Foundation\\": "app/Extend/Instances/Illuminate/Foundation"
		},
		"files": [
			"app/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSP\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
		]
	}
}
